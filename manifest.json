{
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "name": "Cat Hub",
  "description": "Your oasis for everything cats",
  "version": "1.0",

  "browser_action": {
    "default_icon": "extension_icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["cat_facts.js"]
  },
  "permissions": [
    "alarms",
    "http://catfacts-api.appspot.com/*"
  ]
}
